{
  "name": "first-project",
  "version": "1.0.0",
  "description": "A native application built with NativeScript-Vue",
  "author": "miguelcabreja90 <miguel.cabreja90@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --env.tnsAction build",
    "build:android": "npm run build -- --env.android",
    "build:ios": "npm run build -- --env.ios",
    "debug": "webpack --watch --env.tnsAction debug",
    "debug:android": "npm run debug -- --env.android",
    "debug:ios": "npm run debug -- --env.ios",
    "watch": "webpack --watch --env.tnsAction run",
    "watch:android": "npm run watch -- --env.android",
    "watch:ios": "npm run watch -- --env.ios",
    "clean": "rimraf dist"
  },
  "nativescript": {
    "id": "org.softdevel.first",
    "templateVersion": "v2",
    "tns-android": {
      "version": "6.1.0"
    },
    "tns-ios": {
      "version": "6.1.0"
    }
  },
  "dependencies": {
    "nativescript-gradient": "^2.0.1",
    "nativescript-loading-indicator": "^2.5.2",
    "nativescript-theme-core": "^1.0.6",
    "nativescript-ui-sidedrawer": "^7.0.1",
    "nativescript-vue": "^2.4.0",
    "tns-core-modules": "^6.0.0",
    "vue-class-component": "^7.1.0",
    "vue-property-decorator": "^8.2.2",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.2",
    "nativescript-dev-webpack": "^1.0.0",
    "nativescript-vue-template-compiler": "^2.0.0",
    "nativescript-worker-loader": "~0.9.0",
    "node-sass": "^4.13.1",
    "tns-platform-declarations": "^6.0.0",
    "typescript": "^3.2.4",
    "vue": "^2.5.22",
    "vue-loader": "^15.4.0"
  }
}
